{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "website",
			"image": "mrdr/a4-rails",
			"hostname": "website",
			"essential": false,
			"memory": 128
		},
		{
			"name": "sidekiq",
			"image": "mrdr/a4-sidekiq",
			"hostname": "sidekiq",
			"essential": false,
			"memory": 128
		},
		{
			"name": "cable",
			"image": "mrdr/a4-cable",
			"hostname": "cable",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "mrdr/a4-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
		"links": ["website"],
		"memory": 128
		}
	]
}